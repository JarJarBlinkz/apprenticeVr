{
  "name": "apprenticevr",
  "version": "1.0.1",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "electron-vite build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "publish": "electron-builder --publish always",
    "tag-version": "VERSION=$(node -p \"require('./package.json').version\") && TAG_NAME=\"v$VERSION\" && (git rev-parse $TAG_NAME >/dev/null 2>&1 || git tag $TAG_NAME) && echo \"Tag $TAG_NAME created or already exists.\""
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^3.3.8",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@fluentui/react-components": "^9.64.0",
    "@fluentui/react-icons": "^2.0.300",
    "@fluentui/react-popover": "^9.10.7",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.8",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "bluebird": "^3.7.2",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.6.2",
    "execa": "^9.5.3",
    "match-sorter": "^8.0.1",
    "mri": "^1.2.0",
    "node-fetch": "^3.3.2",
    "node-stream-zip": "^1.15.0",
    "node-sys": "^1.2.4",
    "node-unar": "^1.0.8",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bluebird": "^3.5.42",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/when": "^2.4.41",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
